#: src/core/components/InstallButton/index.js:72
msgid "Install Theme"
msgstr "테마 설치"

#: src/core/components/InstallButton/index.js:88
msgid "Add to Firefox"
msgstr "Firefox에 추가"

#: src/core/components/InstallSwitch/index.js:54
msgid "Downloading %(name)s."
msgstr "%(name)s를 다운로드 중입니다."

#: src/core/components/InstallSwitch/index.js:57
