#~ msgstr "Klikniśo how, aby kuždy cas na %(name)s pśistup měł."

#~ msgid "Ok"
#~ msgstr "W pórěźe"

#~ msgid "Preview %(name)s"
#~ msgstr "Pśeglěd za %(name)s"
